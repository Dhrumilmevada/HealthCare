<?xml version="1.0" encoding="utf-8"?>
<android.support.v4.widget.DrawerLayout xmlns:android="http://schemas.android.com/apk/res/android"
xmlns:tools="http://schemas.android.com/tools"
xmlns:app="http://schemas.android.com/apk/res-auto"
android:id="@+id/drawer_lay_yoga"
android:layout_width="match_parent"
android:layout_height="match_parent"
tools:context="com.example.dhrumil.healthcare.homePage.HomePage">

<RelativeLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/rel_lay_yoga_content">

    <include
        layout="@layout/reset_toolbar_layout"
        android:id="@+id/tool_bar_yoga">
    </include>

    <android.support.v7.widget.RecyclerView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/rec_view_youtube_yoga_thumbnail"
        android:layout_below="@id/tool_bar_yoga">
    </android.support.v7.widget.RecyclerView>

</RelativeLayout>

<android.support.design.widget.NavigationView
    android:layout_width="290dp"
    android:layout_height="match_parent"
    android:id="@+id/nav_view_yoga"
    android:layout_gravity="start"
    >
</android.support.design.widget.NavigationView>

</android.support.v4.widget.DrawerLayout>