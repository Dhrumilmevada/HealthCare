<?xml version="1.0" encoding="utf-8"?>
<android.support.v4.widget.DrawerLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/drawer_lay_case_paper"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.dhrumil.healthcare.homePage.HomePage">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/rel_lay_case_paper_content">


        <include
            layout="@layout/reset_toolbar_layout"
            android:id="@+id/tool_bar_case_paper">
        </include>

        <android.support.v7.widget.RecyclerView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/rec_view_case_paper"
            android:layout_below="@id/tool_bar_case_paper">
        </android.support.v7.widget.RecyclerView>

        <android.support.design.widget.FloatingActionButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/float_btn_case_paper"
            android:layout_alignParentBottom="true"
            android:layout_alignParentEnd="true"
            android:layout_margin="40dp"
            android:src="@drawable/ic_add_black_24dp"
            android:backgroundTint="@color/tool_bar_color"
            app:borderWidth="0dp"
            android:clickable="true" />

    </RelativeLayout>

    <android.support.design.widget.NavigationView
        android:layout_width="290dp"
        android:layout_height="match_parent"
        android:id="@+id/nav_view_case_paper"
        android:layout_gravity="start"
        >
    </android.support.design.widget.NavigationView>

</android.support.v4.widget.DrawerLayout>